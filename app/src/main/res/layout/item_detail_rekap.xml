<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="5dp"
    app:cardCornerRadius="5dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tittle_jam_login_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Jam Login"
            android:layout_margin="7dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/tittle_jam_logout_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Jam Logout"
            android:layout_margin="7dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tittle_jam_login_rekap" />

        <TextView
            android:id="@+id/tittle_tanggal_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tanggal"
            android:layout_margin="7dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tittle_jam_logout_rekap" />

        <TextView
            android:id="@+id/tittle_lokasi_latitude_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Latitude"
            android:layout_margin="7dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tittle_tanggal_rekap" />

        <TextView
            android:id="@+id/tittle_lokasi_longitude_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Longitude"
            android:layout_margin="7dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tittle_lokasi_latitude_rekap" />

        <TextView
            android:id="@+id/txt_jam_login_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:layout_margin="7dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <TextView
            android:id="@+id/txt_jam_logout_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2"
            android:layout_margin="7dp"
            app:layout_constraintTop_toBottomOf="@id/txt_jam_login_rekap"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/txt_tanggal_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"
            android:layout_margin="7dp"
            app:layout_constraintTop_toBottomOf="@id/txt_jam_logout_rekap"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/txt_lokasi_latitude_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4"
            android:layout_margin="7dp"
            app:layout_constraintTop_toBottomOf="@id/txt_tanggal_rekap"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/txt_lokasi_longitude_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"
            android:layout_margin="7dp"
            app:layout_constraintTop_toBottomOf="@id/txt_lokasi_latitude_rekap"
            app:layout_constraintEnd_toEndOf="parent"/>

        <TextView
            android:id="@+id/tittle_siswa_diajar_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Siswa yang Akan Diajar"
            android:layout_margin="7dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tittle_lokasi_longitude_rekap"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/txt_siswa_diajar_rekap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"
            android:layout_margin="7dp"
            app:layout_constraintTop_toBottomOf="@id/txt_lokasi_longitude_rekap"
            app:layout_constraintEnd_toEndOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>